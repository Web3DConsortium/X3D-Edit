  <Scene>
    <!-- Example scene to illustrate X3D nodes and fields (XML elements and attributes) -->
    <WorldInfo DEF='WorldInfoPrimary' info='"Example scene to illustrate a simple X3D4 model"' title='Hello World X3D4!'/>
    <WorldInfo DEF='WorldInfoSecondary' title='test HTML CSS fields'/>
    <NavigationInfo type='"EXAMINE" "ANY"'/>
    <Group>
      <Viewpoint DEF='ViewUpClose' centerOfRotation='0 -1 0' description='Hello world!' position='0 -1 7'/>
      <Transform rotation='0 1 0 3'>
        <Shape>
          <Sphere/>
          <Appearance>
            <!-- https://htmlcolorcodes.com/colors/off-white -->
            <Material DEF='MaterialOffWhite' diffuseColor='0.980392 0.976471 0.964706'/>
            <ImageTexture DEF='ImageCloudlessEarth' url='"https://www.web3d.org/x3d/content/examples/earth-topo.png" "https://www.web3d.org/x3d/content/examples/earth-topo.jpg" "https://www.web3d.org/x3d/content/examples/earth-topo-small.gif"'/>
          </Appearance>
        </Shape>
      </Transform>
      <Transform translation='0 -2 0'>
        <Shape>
          <Text DEF='TextMessage' string='"Hello" "X3D 4!"'>
            <FontStyle justify='"MIDDLE" "MIDDLE"'/>
          </Text>
          <Appearance>
            <Material USE='MaterialOffWhite'/>
          </Appearance>
        </Shape>
      </Transform>
    </Group>
  </Scene>
